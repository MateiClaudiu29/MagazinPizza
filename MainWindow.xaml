<Window x:Class="MagazinPizza.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:MagazinPizza"
        mc:Ignorable="d"
        Title="Gestiune Magazin" Height="450" Width="287">
    <Grid Background="{DynamicResource {x:Static SystemColors.ActiveCaptionBrushKey}}">
        <Button Content="Adauga Produse" HorizontalAlignment="Left" Margin="83,33,0,0" VerticalAlignment="Top" Width="105" Height="44" Click="Button_Click"/>
        <Button Content="Gestiune Catalog" HorizontalAlignment="Left" Margin="83,128,0,0" VerticalAlignment="Top" Width="105" Height="44" Click="Button_Click_1"/>
        <Button Content="Casierie" HorizontalAlignment="Left" Margin="10,335,0,0" VerticalAlignment="Top" Width="259" Height="61" FontSize="18" Click="Button_Click_2"/>
        <Button Content="Produse Vandute" HorizontalAlignment="Left" Height="44" Margin="83,228,0,0" VerticalAlignment="Top" Width="105" Click="Button_Click_3"/>
        <Rectangle Fill="{DynamicResource {x:Static SystemColors.ControlLightLightBrushKey}}" HorizontalAlignment="Left" Height="7" Margin="0,299,0,0" Stroke="Black" VerticalAlignment="Top" Width="279"/>

    </Grid>
</Window>
